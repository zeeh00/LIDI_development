<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.reactions.ReactionFailedFragment">

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">
        <ImageButton
            android:background="@drawable/rectangle_border_gray_200_radius_27_5"
            android:id="@+id/btnArrowleft"
            android:layout_height="@dimen/_55pxh"
            android:layout_marginBottom="@dimen/_5pxv"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="@dimen/_55pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:paddingHorizontal="@dimen/_18pxh"
            android:paddingVertical="@dimen/_18pxv"
            android:scaleType="fitCenter"
            android:clickable="true"
            android:src="@drawable/img_arrowleft_bluegray_800"/>
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageCompleteOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_140pxh"
                android:layout_marginEnd="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_80pxv"
                android:layout_width="@dimen/_140pxh"
                android:scaleType="fitXY"
                android:src="@drawable/reaction_failed"
                tools:ignore="ContentDescription" />
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtCONGRATULATIONSOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:text="Failed to Add Data!"
                style="@style/txtSFProDisplaybold24"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtIDN4"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_85pxv"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:text="ID: Not Generated!"
                style="@style/txtSFProDisplaybold20"/>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>